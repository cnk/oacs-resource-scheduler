<?xml version="1.0"?>
<queryset>
	<fullquery name="get_cal_filters">
	<querytext>
		select f.filter_name, f.description, f.cal_filter_id 
		from ctrl_calendar_filters f, acs_objects o
		where f.cal_filter_id = o.object_id 
		and cal_id = :cal_id $filter
		order by f.filter_name
        </querytext>
	</fullquery>
</queryset>
